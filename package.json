    "lint": "eslint ./src",
    "lint-fix": "eslint ./src --fix",
    "test": "nyc mocha --require babel-core/register",
    "lcov": "http-server ./coverage/lcov-report/",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build"
  },
  "keywords": [
    "vobees"
  ],
  "author": "vobi.io",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/vobi-io/vobees"
  },
  "bin": {
    "vobees": "./build/index.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bluebird": "^3.5.3",
    "colors": "^1.3.2",
    "commander": "^2.18.0",
    "download": "^7.1.0",
    "download-file": "^0.1.5",
    "http": "0.0.0",
    "https": "^1.0.0",
    "inquirer": "^6.2.0",
    "mv": "^2.1.1",
    "p-iteration": "^1.1.7",
    "replace": "^1.0.0",
    "unzip": "^0.1.11",
    "lodash": "^4.17.11",
    "@babel/runtime": "^7.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
