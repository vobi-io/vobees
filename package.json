  "description": "Vobi Framework",
  "main": "./build/index.js",
  "scripts": {
    "transpile": "npx babel --presets @babel/preset-env ./src --out-dir ./build  --plugins @babel/plugin-transform-runtime --copy-files --ignore ./src/templates/modules ",
